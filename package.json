{
  "name": "greensteps",
  "version": "1.0.0",
  "description": "A Sustainability & Eco-Friendly Lifestyle Tracker",
  "main": "server/server.js",
  "type": "module",
  "scripts": {
    "start": "node server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run client\" \"nodemon server/server.js\"",
    "build": "npm run build --prefix client",
    "test": "echo \"No tests specified\" && exit 0",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "install": "cd client && npm install && cd ../server && npm install"
  },
  "keywords": [
    "sustainability",
    "eco-friendly",
    "tracker",
    "mern",
    "graphql"
  ],
  "author": "Pink727",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "apollo-server-express": "^2.19.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "cypress": "^9.0.0"
  }
}